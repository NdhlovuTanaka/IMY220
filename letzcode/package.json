{
  "name": "letzcode",
  "version": "1.0.0",
  "description": "A modern version control website for code collaboration and project management",
  "main": "backend/src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "webpack --mode development --watch",
    "dev:backend": "nodemon backend/src/server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "webpack --mode production",
    "build:backend": "babel backend/src -d backend/dist --copy-files",
    "start": "npm run build && node backend/src/server.js",
    "start:prod": "node backend/dist/server.js",
    "clean": "rimraf frontend/public/bundle.js backend/dist",
    "lint": "eslint frontend/src backend/src --ext .js,.jsx",
    "lint:fix": "eslint frontend/src backend/src --ext .js,.jsx --fix",
    "format": "prettier --write \"frontend/src/**/*.{js,jsx,css}\" \"backend/src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t letzcode-app .",
    "docker:run": "docker run -p 3000:3000 letzcode-app",
    "docker:dev": "docker-compose up --build"
  },
  "keywords": [
    "version-control",
    "code-collaboration",
    "react",
    "express",
    "mongodb",
    "file-sharing"
  ],
  "author": "Ndhlovu Tanaka u22610792",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "multer": "^1.4.5-lts.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.1",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.4.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "axios": "^1.7.7",
    "date-fns": "^3.6.0",
    "classnames": "^2.5.1",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "prismjs": "^1.29.0",
    "next": "15.5.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.542.0",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "file-loader": "^6.2.0",
    "url-loader": "^4.1.1",
    "html-webpack-plugin": "^5.6.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "nodemon": "^3.1.4",
    "concurrently": "^9.0.1",
    "rimraf": "^6.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "jest": "^29.7.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.5.0",
    "cross-env": "^7.0.3",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@types/node": "24.3.1",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "postcss": "8.5.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NdhlovuTanaka/IMY220"
  }
}
